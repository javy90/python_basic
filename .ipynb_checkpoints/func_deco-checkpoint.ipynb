   "source": [
    "#함수 데코레이터\n",
    "def test(function):\n",
    "    def wrapper():\n",
    "        print(\"함수 시작\")\n",
    "        function()\n",
    "        print(\"함수 종료\")\n",
    "    return wrapper\n",
    "\n",
    "#데코레이터 함수정의\n",
    "@test #@가 들어가면 데코 @함수명 데코레이터를 먼저실행\n",
    "def hello():\n",
    "    print(\"hello\")\n",
    "# 함수 호출\n",
    "hello()\n"
   ]
